/clients/{clientDedupId}/global-enrollments/{global-enrollmentId}:
  get:
    description: Get selected global enrollment
    responses:
      200:
        body:
          application/json:
            example:
                  {
                   "globalEnrollment": {
                      "globalEnrollmentId":"5bf2063e-0e37-4604-886b-6d95dc98bede",
                      "enrollments": [ {
                                       "enrollmentId":"d12a5b43-9411-4fe3-8cd3-0778459d0911",
                                        "schema":"2014",
                                        "link":"www.hmislynk.com/client-api/rest/v2014/clients/5bf2063e-0e37-4604-886b-6d95dc98bede/enrollments/d12a5b43-9411-4fe3-8cd3-0778459d0911"
                                      },
                                      {
                                        "enrollmentId":"d12a5b43-9411-4fe3-8cd3-0778459d0085",
                                         "schema":"humanservices",
                                         "link":"www.hmislynk.com/humanservices-api/rest/clients/5bf2063e-0e37-4604-886b-6d95dc98bede/enrollments/d12a5b43-9411-4fe3-8cd3-0778459d0085"
                                       }
                      ]
                   }
                  }
  delete:
    description: Delete selected global enrollment
/clients/{clientDedupId}/global-enrollments/{global-enrollmentId}/enrollments:
  put:
    description: Add enrollment to selected global enrollment
    body:
      application/json:
        example:
              {
                 "enrollments":{
                    "enrollment":[
                       {
                          "enrollmentId":"d12a5b43-9411-4fe3-8cd3-0778459d0911",
                          "schema":"2014"
