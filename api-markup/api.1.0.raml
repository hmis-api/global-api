      200:
        body:
          application/json:
/clients/{clientDedupId}/global-enrollments/{global-enrollmentId}/enrollments/{enrollmentId}:
  delete:
    description: Remove enrollment from selected global enrollment
    responses:
      200:
        body:
          application/json:
