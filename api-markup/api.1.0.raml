          application/json:
            example:
                  {
                     "globalEnrollments":{
                        "pagination":{
                           "from":0,
                           "returned":1,
                           "total":1,
                           "maximum":30,
                           "sort":{

                           }
                        },
                        "globalEnrollments":[
                           {
                              "globalEnrollmentId":"5bf2063e-0e37-4604-886b-6d95dc98bede",
